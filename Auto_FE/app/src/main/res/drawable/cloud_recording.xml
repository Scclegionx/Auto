<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">
    
    <gradient
        android:startColor="#FF6B6B"
        android:centerColor="#FF8E8E"
        android:endColor="#FFB3B3"
        android:angle="135"
        android:type="radial" />
    
    <stroke
        android:width="3dp"
        android:color="#FFFFFF"
        android:dashWidth="5dp"
        android:dashGap="3dp" />
    
    <size
        android:width="140dp"
        android:height="140dp" />
    
</shape>
